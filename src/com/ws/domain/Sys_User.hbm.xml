<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ws.domain" >
	<class name="Sys_User" table="sys_user" >
		<id name="user_id"  >
			<!-- generator:主键生成策略 -->
			<generator class="native"></generator>
		</id>
		<property name="user_userName" column="user_userName" ></property>
		<property name="user_password" column="user_password" ></property>
		<property name="user_email" column="user_email" ></property>
		<property name="user_phone" column="user_phone" ></property>
		<property name="user_sex" column="user_sex" ></property>
		<property name="user_province" column="user_province" ></property>
		<property name="user_city" column="user_city" ></property>
		<property name="user_area" column="user_area" ></property>
		<property name="user_code" column="user_code" ></property>
		<property name="user_state" column="user_state" ></property>



	</class>
</hibernate-mapping>